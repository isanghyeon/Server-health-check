{
  "memory": {
    "usage": {
      "MemoryUsed": "cat /proc/meminfo | grep Mem",
      "StorageUsed": "df -h"
    }
  },

  "docker-inspect": {
    "docker-container": {
      "container-id": "docker ps -a | awk '{print $1}'",
      "container-name": "docker ps -a | awk '{print $7}'",
      "container-live": "docker ps -a | awk '{print $4}'"
    }
  }

}