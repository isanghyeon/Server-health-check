{
  "memory": {
    "usage": {
      "MemoryUsed": "cat /proc/meminfo | grep Mem",
      "StorageUsed": "df -h"
    }
  },

  "docker-inspect": {
    "docker-container": {
      "container-id": "docker ps -a | grep \"Exited\" | awk '{print $1}'",
      "container-name": "docker ps -a | grep \"Exited\" | awk '{print $12}'",
      "container-health-check": "docker ps -a | grep \"Exited\"",
      "container-time": "docker ps -a | grep \"Exited\" | awk '{print $7, $8}'"
    }
  }

}